{
  "name": "sonar-coupling-plugin",
  "license": "LGPL-3.0",
  "version": "7.0.0",
  "devDependencies": {
    "@babel/core": "7.22.11",
    "@babel/plugin-proposal-private-property-in-object": "^7.2.5",
    "autoprefixer": "10.4.15",
    "babel-jest": "29.6.4",
    "babel-loader": "^8.2.3",
    "babel-preset-react-app": "^10.0.1",
    "cross-env": "7.0.3",
    "cross-spawn": "7.0.3",
    "css-loader": "6.8.1",
    "detect-port": "1.5.1",
    "dotenv": "16.3.1",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "expose-loader": "4.1.0",
    "express": "4.18.2",
    "express-http-proxy": "1.6.3",
    "filesize": "10.0.12",
    "find-cache-dir": "5.0.0",
    "gzip-size": "7.0.0",
    "imports-loader": "4.0.1",
    "jest": "29.6.4",
    "json-loader": "0.5.7",
    "path-exists": "2.1.0",
    "postcss-loader": "7.3.3",
    "prettier": "3.0.3",
    "react": "18.2.0",
    "react-addons-shallow-compare": "15.6.3",
    "react-dev-utils": "0.5.2",
    "react-dom": "18.2.0",
    "react-router": "6.15.0",
    "react-transform-hmr": "1.0.4",
    "recursive-readdir": "2.2.3",
    "rimraf": "5.0.1",
    "script-loader": "0.7.2",
    "strip-ansi": "7.1.0",
    "style-loader": "3.3.3",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "5.9.0"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js",
    "mystart": "NODE_ENV=dev BABEL_ENV=dev webpack serve --open --config conf/webpack.config.js"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "jest": {
    "coverageDirectory": "<rootDir>/target/coverage",
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/tests"
    ],
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(hbs|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/conf/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/conf/jest/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/conf/jest/SetupTestEnvironment.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/scripts",
      "<rootDir>/conf"
    ]
  },
  "dependencies": {
    "backbone": "^1.5.0",
    "chart.js": "^4.4.0",
    "chartjs-plugin-zoom": "^2.0.1",
    "jquery": "^3.7.1",
    "react-chartjs-2": "^5.2.0",
    "underscore": "^1.13.6"
  }
}
